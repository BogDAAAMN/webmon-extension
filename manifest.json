{
  "manifest_version": 2,
  "name": "Web Monetization extension",
  "description": "This extension doesnt do much yet.",
  "version": "1.0",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "./popup/popup.html"
  },
  "content_scripts":[{
    "js":[ "injectbutton.js"], 
    "matches":["*://www.youtube.com/watch*"],
    "css": ["styles.css"]
    }], 
    
  "permissions": [
    "https://*/*",  
    "http://*/*",
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/", 
    "storage"

  ]
}

